{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
  "compilerOptions": {
    "baseUrl": "../../",
    "paths": {
      "editor/bootstrap": ["apps/editor/src/bootstrap.tsx"],
      "store_mf/exports": ["apps/store_mf/src/exports.ts"]
    },
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "inlineSources": false,
    "isolatedModules": true,
    "moduleResolution": "node",
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "preserveWatchOutput": true,
    "skipLibCheck": true,
    "strict": true
  },
  "references": [{ "path": "../pixi" }],
  "exclude": ["node_modules"]
}
